<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:height="10dp"
        android:width="10dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="72"
            android:pivotY="0">
            <shape
                android:shape="rectangle" >
                <solid
                    android:color="#dcdcdc" />
            </shape>
        </rotate>
    </item>

</layer-list>